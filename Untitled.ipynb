    "def reverseWords(s):\n",
    "    words = s.split(' ')\n",
    "    new = []\n",
    "    for word in words:\n",
    "        print(word)\n",
    "        rev = word[::-1]\n",
    "        print(rev)\n",
    "        new.append(rev)\n",
    "    return ' '.join(new)"
   ]
